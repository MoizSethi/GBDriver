Qoutes_api:
- Rides_info_api:
  - http://localhost:5000/api/ride/create
  - http://localhost:5000/api/ride/all

- Guest_api:
  - http://localhost:5000/api/guest/register

- User_Register_api:
  - http://localhost:5000/api/user/register
  - http://localhost:5000/api/user/login
  
- Vehicle_api:
  - http://localhost:5000/api/vehicle/add
  - http://localhost:5000/api/vehicle/all
- Driver_api:
  - http://localhost:5000/api/driver/register
  - http://localhost:5000/api/driver/login
  - http://localhost:5000/api/driver/all
  - http://localhost:5000/api/driver/approve/:driverId
  - http://localhost:5000/api/driver/pending
- Admin_api:
  - http://localhost:5000/api/admin/create
  - http://localhost:5000/api/admin/login
  - http://localhost:5000/api/admin/all

- Database_Migrations:
  - npx sequelize-cli db:migrate --env development